					<string>ORIGIN_PATH=&quot;/Applications/Xcode.app/___VARIABLE_basePlatformPath___/Project Templates&quot;
ORIGIN_FOLDER=&quot;___VARIABLE_basePlatformFolder___&quot;
if [ ! ${#ORIGIN_FOLDER} -eq 0 ]; then
    ORIGIN_PATH=&quot;${ORIGIN_PATH}/${ORIGIN_FOLDER}&quot;
fi
GROUP_NAME=&quot;___VARIABLE_groupName___&quot;
GROUP_PATH=&quot;${ORIGIN_PATH}/${GROUP_NAME}&quot;
LOCAL_PATH=&quot;${PROJECT_DIR}/${PROJECT_NAME}&quot;
TEMPLATE_NAME=&quot;${PROJECT_NAME}.xctemplate&quot;
PASSWORD_FILE=&quot;${PROJECT_DIR}/${PROJECT_NAME}/Password&quot;

createGroupFun(){
    if [ -d &quot;${GROUP_PATH}&quot; ]; then
        echo &quot;[Group Folder Exsit]&quot;
    else
        echo &quot;[Group Folder NotFound]&quot;
        if [ $# -eq 0 ]; then
#apple script
#----------------------
originFile=&quot;${ORIGIN_PATH}&quot;    
groupName=&quot;${GROUP_NAME}&quot;
osascript &lt;&lt;EOF
    tell application &quot;Finder&quot;
        make folder at (POSIX file &quot;$originFile&quot;) with properties {name:&quot;$groupName&quot;}
    end tell
EOF
#----------------------
        else
            PASSWORD=$1
            echo &quot;${PASSWORD}&quot; | sudo -S mkdir &quot;${GROUP_PATH}&quot;
        fi
    fi
}

removeFun(){
    if [ -d &quot;${GROUP_PATH}/${TEMPLATE_NAME}&quot; ]; then   
        if [ $# -eq 0 ]; then
#apple script
#----------------------
originFile=&quot;${GROUP_PATH}/${TEMPLATE_NAME}&quot;    
osascript &lt;&lt;EOF
    tell application &quot;Finder&quot;
            delete POSIX file &quot;$originFile&quot;
    end tell
EOF
#----------------------
        else
            PASSWORD=$1
            echo &quot;${PASSWORD}&quot; | sudo -S rm -fr &quot;${GROUP_PATH}/${TEMPLATE_NAME}&quot;
        fi
    else
        echo &quot;[Origin File NotFound]&quot;
    fi 
}

duplicateFun(){
    if [ $# -eq 0 ]; then
#apple script
#----------------------
localFile=&quot;${LOCAL_PATH}/${TEMPLATE_NAME}&quot;
originFloder=&quot;${GROUP_PATH}&quot;
osascript &lt;&lt;EOF
tell application &quot;Finder&quot;
    duplicate POSIX file &quot;$localFile&quot; to POSIX file &quot;$originFloder&quot;
end tell
EOF
#---------------------- 
    else
        PASSWORD=$1
        echo &quot;${PASSWORD}&quot; | sudo -S cp -fr\
        &quot;${LOCAL_PATH}/${TEMPLATE_NAME}&quot;\
        &quot;${GROUP_PATH}/${TEMPLATE_NAME}&quot;
    fi
}

syncFun(){
    if [ $# -eq 0 ]; then  
        createGroupFun  
        removeFun
        duplicateFun
    else
        PASSWORD=$1
        createGroupFun &quot;${PASSWORD}&quot;
        removeFun &quot;${PASSWORD}&quot; 
        duplicateFun &quot;${PASSWORD}&quot;
    fi
}

if [ -e &quot;${PASSWORD_FILE}&quot; ]; then
    PASSWORD=`cat &quot;${PASSWORD_FILE}&quot;`
    if [ ${#PASSWORD} -eq 0 ]; then
        echo &quot;[Password not set]&quot;
        syncFun
    else
        echo &quot;[Password has set]&quot;
        syncFun &quot;${PASSWORD}&quot; 
    fi
else
    echo &quot;[Password file not set]&quot; 
    syncFun
fi
